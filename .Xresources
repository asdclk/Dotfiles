URxvt*font: xft:DejaVu Sans Mono:size=9:antialias=true
URxvt*saveLines: 10000
URxvt*scrollBar: false
URxvt*letterSpace: -2
URxvt*termName: rxvt-unicode

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

xterm*facename: xft:DejaVu Sans Mono:size=9:antialias:true
xterm*font: -*-fontawesome-medium-*-normal-*-*-*-100-100-*-*-*-*
xterm*locale: true
xterm*saveLines: 10000
xterm*scrollBar: false
xterm*letterSpace: -2
xterm*termName: xterm-256color
xterm.url-select.launcher:  firefox
xterm.url-select.underline: true


